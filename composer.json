{
    "name": "j1nz/slem-stater",
    "description": "A template starter structure for Slim 3.x",
    "keywords": ["template", "slim", "boilerplate"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.8",
        "slim/twig-view": "^2.2",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "slim/http-cache": "^0.4.0",
        "monolog/monolog": "^1.23",
        "nochso/html-compress-twig": "^2.0",
        "slim/flash": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
